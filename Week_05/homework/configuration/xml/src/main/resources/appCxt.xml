<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="student01" class="entity.Student">
        <property name="age" value="20"/>
        <property name="name" value="Wang"/>
        <property name="course" ref="course01"/>
    </bean>

    <bean id="course01" class="entity.Course">
        <property name="courseId" value="01"/>
        <property name="courseName" value="Math"/>
    </bean>
</beans>